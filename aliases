# Get External IP / Internet Speed
alias myip="curl https://ipinfo.io/json" # or /ip for plain-text ip
alias speedtest="curl -s https://raw.githubusercontent.com/sivel/speedtest-cli/master/speedtest.py | python -"

# Quickly serve the current directory as HTTP
alias serve='ruby -run -e httpd . -p 8000'  # Or python -m SimpleHTTPServer :)

alias unity2019='/Applications/Unity/Hub/Editor/2019.4.29f1/Unity.app/Contents/MacOS/Unity'
alias unity2020='/Applications/Unity/Hub/Editor/2020.3.20f1/Unity.app/Contents/MacOS/Unity'
alias unity20-open='unity2020 -projectPath'
alias unity19-open='unity2019 -projectPath'

alias jn='jupyter notebook'

alias wow="echo wow: command not found"